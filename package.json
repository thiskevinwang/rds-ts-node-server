{
  "name": "rds-ts-node-server",
  "version": "0.1.1",
  "description": "An Apollo Server that uses TypeORM to connect to an RDS-postgres instance",
  "author": "kwangsan@gmail.com",
  "main": "index.ts",
  "repository": "thiskevinwang/rds-ts-node-server",
  "license": "MIT",
  "scripts": {
    "start": "ts-node index.ts",
    "nodemon": "nodemon -e ts -w ./src -x yarn start",
    "dev": "ts-node-dev --debug --no-notify --tree-kill --respawn --transpile-only index.ts",
    "dev:container": "docker run --rm -d -it -p 8000:8000 amazon/dynamodb-local",
    "format": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/ms": "^0.7.31",
    "@types/node": "12.12.16",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "apollo-server": "^2.19.0",
    "aws-sdk": "^2.592.0",
    "axios": "^0.21.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "graphql": "15.4.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.11.0",
    "lodash": "^4.17.19",
    "ms": "^2.1.2"
  },
  "engines": {
    "node": "12.x"
  }
}

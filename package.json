{
  "name": "rds-ts-node-server",
  "version": "0.1.1",
  "description": "An Apollo Server that uses TypeORM to connect to an RDS-postgres instance",
  "author": "kwangsan@gmail.com",
  "main": "index.ts",
  "repository": "thiskevinwang/rds-ts-node-server",
  "license": "MIT",
  "scripts": {
    "start": "ts-node index.ts",
    "dev": "ts-node-dev --no-notify --respawn --transpileOnly index.ts",
    "dev:watch": "concurrently \"pgtyped -w -c config.json\" \"ts-node-dev --no-notify --respawn --transpileOnly index.ts\"",
    "dev:migrations": "ts-node scripts/migration.ts",
    "dev:container": "docker run --rm --name postboi -e POSTGRES_PASSWORD=mysecretpassword -d -p 8080:5432 postgres:13-alpine",
    "dev:generate": "ts-node scripts/generateAttemptsSql.ts",
    "watch": "pgtyped -w -c config.json",
    "format": "prettier --write src/**/*.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/common-tags": "^1.8.0",
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.149",
    "@types/ms": "^0.7.31",
    "@types/node": "12.12.16",
    "@types/pg": "^7.14.4",
    "prettier": "^2.0.2",
    "ts-node": "8.5.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@pgtyped/cli": "^0.8.1",
    "@pgtyped/query": "^0.8.1",
    "apollo-server": "^2.16.0",
    "aws-sdk": "^2.592.0",
    "bcryptjs": "^2.4.3",
    "common-tags": "^1.8.0",
    "concurrently": "^5.2.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "ms": "^2.1.2",
    "pg": "^8.3.0",
    "postgres-migrations": "^4.0.3",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.21"
  },
  "engines": {
    "node": "12.x"
  }
}
